#include <stdio.h>

/**
 * main - "Function that prints 1-100 excepts for multiples of 3 & 5, print 'Fizz' & 'Buzz' for them respectively, 'FizzBuzz' for multiples of both."
 *
 *
 * Return: Always 0
 */
int main(void)
{
	int num;

	for (num = 1; num <= 100; num++)
	{
		if ((num % 3) == 0 && (num % 5) == 0)
			printf("FizzBuzz");

		else if ((num % 5) == 0)
			printf("Fizz");

		else 
			printf("%d", num);

		if (num == 100)
			continue;
		print(" ");
	}
	printf('\n');

	return (0);
}


